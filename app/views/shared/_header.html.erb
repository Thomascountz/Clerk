<%= nav_bar static: :top, brand: "Clerk", responsive: true do %>
  <%= menu_group pull: :right do %>
    <% if current_user %>
      <%= menu_item "Sign out", destroy_user_session_path, method: :delete %>
    <% else %>
      <%= menu_item "Sign in", new_user_session_path %>
      <%= menu_item "Create an Account", new_user_registration_path %>
    <% end %> 
  <% end %>
<% end %>
